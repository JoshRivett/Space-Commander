            <string>if keyboard_check(ord('R')) {
    room_restart()
}
if keyboard_check(ord('M')) {
    window_set_fullscreen(false)
    score = 0
    room = MainMenu
}

if keyboard_check(ord('V')) {
    score = score + 100
}

if not(instance_exists(oPlayerShip)) and gameOver = false{
    gameOver = true
    alarm[0] = 3 * room_speed
}

//Aspect Ratio //
if keyboard_check(ord('-')){
    view_xview[0] = 1920
    view_yview[0] = 1080
}else if keyboard_check(ord('=')){
    view_xview[0] = 1280
    view_yview[0] = 1024
}

// View Scaling //
if keyboard_check(ord('1')) {
    view_wview[0] = 1920
    view_hview[0] = 1080
}else if keyboard_check(ord('2')) {
    view_wview[0] = 1440
    view_hview[0] = 810
}else if keyboard_check(ord('3')) {
    view_wview[0] = 960
    view_hview[0] = 540
}

// FPS Options //
if keyboard_check(ord('8')) {
    room_speed = 30
}else if keyboard_check(ord('9')) {
    room_speed = 60
}else if keyboard_check(ord('0')) {
    room_speed = 144
}

if instance_exists(oPlayerShip) {
    if oPlayerShip.sprite_index = sPlayerShip {
        drawCrosshair = false
    } else if oPlayerShip.sprite_index = sBlueShip {
        drawCrosshair = true
    }
}

if score &lt; 150 {
    progress = 30
} else if score &gt;= 150 and score &lt; 300 {
    progress = 55
} else if score &gt;= 300 and score &lt; 600 {
    progress = 80
} else if score &gt;= 600 and score &lt; 1200 {
    progress = 95
} else if score &gt;= 1200 {
    progress = 100
}

if score &gt;= 1500 and boss1 = false {
    bossFight = true
} else if score &gt;= 2500 and boss2 = false {
    bossFight = true
}

if score &gt;= 1500 and not instance_exists(oEnemy) and boss1 = false {
    instance_create(room_width/2, room_height/2, oBoss1Portal)
    boss1 = true
} else if score &gt;= 2500 and not instance_exists(oEnemy) and boss2 = false {
    instance_create(room_width/2, room_height/2, oBoss2Portal)
    boss2 = true
}
