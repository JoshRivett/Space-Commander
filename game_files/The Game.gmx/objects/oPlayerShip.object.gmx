            <string>if keyboard_check(vk_left) or keyboard_check(ord('A')) {
    image_index = image_index + (60 / room_speed)
}
if keyboard_check(vk_right) or keyboard_check(ord('D')) {
    image_index = image_index - (60 / room_speed)
}
if speed &gt; (maxSpeed) {
    speed = (maxSpeed)
} else if (keyboard_check(vk_up) or keyboard_check(ord('W'))) and speed &lt;= (maxSpeed) {
    motion_add((image_index * 6), acceleration)
    instance_create(x,y,oTrailRed)
    instance_create(x,y,oTrailOrange)
    instance_create(x,y,oTrailYellow)
} else if (keyboard_check(vk_down) or keyboard_check(ord('S'))) and speed &lt;= (maxSpeed) {
    motion_add(((image_index * 6) + 180), (acceleration / 2))
}

if keyboard_check_pressed(ord('G')) {
    if keyboard = true {
        sprite_index = sBlueShip
        keyboard = false
        shotCooldownL = false
    } else {
        sprite_index = sPlayerShip
        keyboard = true
    }
}

if keyboard = true {
    if keyboard_check(ord('X')) {
        if shotCooldownL = false {
            bulletL = instance_create(x,y,oLaserShotL)
            
            bulletL.direction = image_index * 6
            bulletL.image_index = image_index
            
            motion_add(((image_index * 6) + 180), (60 / room_speed))
            
            if speed &gt; 0{
                bulletL.speed = (720 / room_speed) + (speed / 2)
            } else {
                bulletL.speed = (720 / room_speed)
            }
    
            bulletL.hspeed = bulletL.hspeed + hspeed
            bulletL.vspeed = bulletL.vspeed + vspeed
            
            shotCooldownL = true
            alarm[0] = (attackSpeed)
        } else if shotCooldownR = false {
            bulletR = instance_create(x,y,oLaserShotR)
            
            bulletR.direction = image_index * 6
            bulletR.image_index = image_index
            
            motion_add(((image_index * 6) + 180), (60 / room_speed))
            
            if speed &gt; 0{
                bulletR.speed = (720 / room_speed) + (speed / 2)
            } else {
                bulletR.speed = (720 / room_speed)
            }
    
            bulletR.hspeed = bulletR.hspeed + hspeed
            bulletR.vspeed = bulletR.vspeed + vspeed
            
            shotCooldownR = true
            alarm[1] = (attackSpeed)
        }
    }
    //if keyboard_check(ord('X')) and shotCooldownR = false {
