    //    alarm[1] = 20
    //}
    
    if keyboard_check_pressed(vk_space){
        ball = instance_create(x,y,oBall)
        ball.direction = image_index * 6
        
        if speed &gt; 0{
            ball.speed = (720 / room_speed) + (speed / 2)
        } else {
            ball.speed = (720 / room_speed)
        }
    
        ball.hspeed = ball.hspeed + hspeed
        ball.vspeed = ball.vspeed + vspeed
    }
} else {
    if mouse_check_button(mb_left)
        if shotCooldownL = false {
            bullet = instance_create(x,y,oLaserShot)
                
            bullet.direction = point_direction(x, y, mouse_x, mouse_y)
            bullet.image_index = point_direction(x, y, mouse_x, mouse_y) / 6
                
            motion_add((point_direction(x, y, mouse_x, mouse_y) + 180), (60 / room_speed))
                
            //if speed &gt; 0{
