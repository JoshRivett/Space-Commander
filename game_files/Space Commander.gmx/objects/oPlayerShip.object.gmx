        <arguments>
          <argument>
            <kind>1</kind>
            <string>if (keyboard_check(vk_left) or keyboard_check(ord('A'))) and inControl = true {
    image_angle = image_angle + 10
}
if (keyboard_check(vk_right) or keyboard_check(ord('D'))) and inControl = true  {
    image_angle = image_angle - 10
}
if speed &gt; (maxSpeed) and inControl = true {
    speed = (maxSpeed)
} else if (keyboard_check(vk_up) or keyboard_check(ord('W'))) and speed &lt;= (maxSpeed) and inControl = true  {
    motion_add((image_angle), acceleration)
    instance_create(x,y,oTrailRed)
    instance_create(x,y,oTrailOrange)
    instance_create(x,y,oTrailYellow)
} else if (keyboard_check(vk_down) or keyboard_check(ord('S'))) and speed &lt;= (maxSpeed) and inControl = true  {
    motion_add((image_angle + 180), (acceleration / 2))
}

if inControl = false {
    image_angle = image_angle + 20
}

if keyboard_check_pressed(ord('G')) {
    if keyboard = true {
        sprite_index = sBlueShip
        keyboard = false
        shotCooldownL = false
    } else {
        sprite_index = sPlayerShip
        keyboard = true
    }
}

if keyboard = true {
    if keyboard_check(ord('X')) {
        if shotCooldownL = false {
            sound_play(snLaser)
            bulletL = instance_create(x,y,oLaserShotL)
            
            bulletL.direction = image_angle
            bulletL.image_angle = image_angle
            
            motion_add((image_angle + 180), (60 / room_speed))
            
            if speed &gt; 0{
                bulletL.speed = (720 / room_speed) + (speed / 2)
            } else {
                bulletL.speed = (720 / room_speed)
            }
    
            bulletL.hspeed = bulletL.hspeed + hspeed
            bulletL.vspeed = bulletL.vspeed + vspeed
            
            shotCooldownL = true
            alarm[0] = (attackSpeed)
        } else if shotCooldownR = false {
            sound_play(snLaser)
            bulletR = instance_create(x,y,oLaserShotR)
            
            bulletR.direction = image_angle
            bulletR.image_angle = image_angle
            
            motion_add((image_angle + 180), (60 / room_speed))
            
            if speed &gt; 0{
                bulletR.speed = (720 / room_speed) + (speed / 2)
            } else {
                bulletR.speed = (720 / room_speed)
            }
    
            bulletR.hspeed = bulletR.hspeed + hspeed
            bulletR.vspeed = bulletR.vspeed + vspeed
            
            shotCooldownR = true
            alarm[1] = (attackSpeed)
        }
    }
    
} else {
    if mouse_check_button(mb_left)
        if shotCooldownL = false {
            bullet = instance_create(x,y,oLaserShot)
                
            bullet.direction = point_direction(x, y, mouse_x, mouse_y)
            bullet.image_angle = point_direction(x, y, mouse_x, mouse_y)
                
            motion_add((point_direction(x, y, mouse_x, mouse_y) + 180), (60 / room_speed))
                
            //if speed &gt; 0{
                //bulletL.speed = (720 / room_speed) + (speed / 2)
            //} else {
                //bulletL.speed = (720 / room_speed)
            //}
        
            bullet.hspeed = bullet.hspeed + hspeed
            bullet.vspeed = bullet.vspeed + vspeed
                
            shotCooldownL = true
            alarm[1] = (attackSpeed)
        }
}
    
if not(instance_exists(oShield)) and shieldSpawning = false{
    shieldSpawning = true
    alarm[2] = 5 * room_speed
}

if instance_exists(oExit) {
    if distance_to_object(oExit) &lt; 400 {
        motion_add(point_direction(x,y,oExit.x,oExit.y), (300 / room_speed))
    }
}

screenWrap()
</string>
